{
  "name": "vue-shop",
  "version": "1.0.0",
  "description": "A Vue.js shop website for learning Vue after Angular",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
