{
  "name": "vue-shop",
  "version": "1.0.0",
  "description": "A Vue.js shop website for learning Vue after Angular",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "start": "powershell -ExecutionPolicy Bypass -File start-full-stack.ps1",
    "start:app": "powershell -ExecutionPolicy Bypass -File start-app.ps1",
    "start:frontend": "vite",
    "start:backend": "cd backend-go && go run ."
  },
  "dependencies": {
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "typescript": "^5.9.2",
    "vite": "^4.4.9",
    "vue-tsc": "^3.0.6"
  },
  "keywords": [
    "vue",
    "shop",
    "learning",
    "angular-migration"
  ],
  "author": "Vue Learner",
  "license": "MIT"
}
